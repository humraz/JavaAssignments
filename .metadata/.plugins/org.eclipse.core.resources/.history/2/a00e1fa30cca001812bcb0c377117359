package com.deloitte;

public class AlternatePrint {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0;i<20;i++)
			{
			
		for(int j=8;j<14;j++)
		{
			System.out.println(i);
		}
		for (int k=1;k<7;k++)
		{
			System.out.println(i);
		}
			}
	}

}
